{
  "name": "enpii-debug",
  "version": "1.0.0",
  "description": "Enpii Debug Package",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --watch --mode development",
    "i18n": "npm run i18n:build",
    "i18n:build": "npm run i18n:pot && ./bin/build_i18n.sh",
    "i18n:pot": "php -d xdebug.max_nesting_level=512 $(which wp) i18n make-pot --exclude=\"node_modules/,languages/,assets/\" --headers='{\"Report-Msgid-Bugs-To\":\"https://github.com/enpiicom/wp-plugin-enpii-debug/issues/new\", \"language-Team\":\"Enpii Team <dev@enpii.com>\"}' . languages/enpii.pot",
    "i18n:json": "$(which wp) i18n make-json languages --no-purge",
    "phpunit": "php -d xdebug.max_nesting_level=512 ./vendor/bin/phpunit",
    "phpcs": "php -d xdebug.max_nesting_level=512 ./vendor/bin/phpcs",
    "phpcbf": "php -d xdebug.max_nesting_level=512 ./vendor/bin/phpcbf"
  },
  "author": "dev@enpii.com",
  "license": "MIT",
  "keywords": [
    "Enpii Base",
    "Laravel",
    "Laravel WordPress",
    "Telescope",
    "Web Tinker",
    "Debug",
    "Enpii Debug"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.8",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "css-loader": "^3.6.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv": "^16.4.5",
    "dotenv-webpack": "^8.1.0",
    "file-loader": "^6.2.0",
    "i": "^0.3.7",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.77.0",
    "sass-loader": "^14.2.1",
    "source-map-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "bootstrap": "^5.3.2",
    "jquery": "^3.5.1",
    "style-loader": "^1.1.3",
    "cross-env": "7.0.3",
    "webpack": "^5.91.0",
    "webpack-build-notifier": "^2.3.0",
    "webpack-cli": "^5.1.4"
  }
}
