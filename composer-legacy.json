{
  "name": "enpii/enpii-debug",
  "type": "wordpress-plugin",
  "description": "Debug plugin to be used with Enpii Base",
  "license": "MIT",
  "authors": [
    {
      "name": "Enpii Team",
      "email": "dev@enpii.com",
      "homepage": "https://team.enpii.com"
    },
    {
      "name": "Trac Nguyen",
      "email": "npbtrac@yahoo.com"
    }
  ],
  "require": {
    "mcaskill/composer-exclude-files": "~3.0.0",
    "laravel/telescope": "~4.1.0",
    "spatie/laravel-web-tinker": "~1.8.4",
    "php": "^7.3|^8.0"
  },
  "config": {
    "php": "7.3",
    "platform-check": false,
    "allow-plugins": {
      "mcaskill/composer-exclude-files": true,
      "composer/installers": true
    },
    "vendor-dir": "./vendor-legacy",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Enpii_Debug\\": "src"
    }
  }
}
